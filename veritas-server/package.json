{
  "name": "@tsed/basic-example",
  "version": "1.0.0",
  "description": "Here an example to configure your server and how to create your first controller.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "^5.36.0",
    "@tsed/core": "5.36.0",
    "@tsed/di": "5.36.0",
    "@tsed/swagger": "5.36.0",
    "@tsed/testing": "5.36.0",
    "@types/swagger-schema-official": "2.0.20",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "6.0.3",
    "express": "4.17.1",
    "method-override": "^3.0.0",
    "node-uuid": "1.4.8",
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@types/chai": "4.2.5",
    "@types/chai-as-promised": "7.1.2",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "1.17.2",
    "@types/method-override": "0.0.31",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.9",
    "@types/request-promise": "4.1.45",
    "@types/sinon": "7.5.0",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "5.0.0",
    "mocha": "6.2.2",
    "nodemon": "1.19.4",
    "nyc": "14.1.1",
    "rimraf": "3.0.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "supertest": "4.0.2",
    "ts-node": "^8.5.2",
    "tslint": "5.20.1",
    "typescript": "^3.7.2"
  }
}
